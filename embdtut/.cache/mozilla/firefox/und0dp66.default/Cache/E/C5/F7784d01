function checkCourseHasUnread() {
  var _e_list = $('.sub_open');
  var _kj_p = [];
  var _kj_t = [];
  var _kj_s = [];
  var _kj_a = [];
  
  var _kj_map = {};
  
  _e_list.each(function(idx, e) {
    var _e = $(this);
    var _kj = _e.attr("kj");
    var _kj_auth = _e.attr("kj_auth");
    if (_kj_auth == 'P') {
      _kj_p.push(_kj);
    } else if (_kj_auth == 'T') {
      _kj_t.push(_kj);
    } else if (_kj_auth == 'S') {
      _kj_s.push(_kj);
    } else if (_kj_auth == 'A') {
      _kj_a.push(_kj);
    }

    _kj_map[_kj_auth + "_" + _kj] = _e;
  });
  
  var _kj_list = "";
  var _prefix_1 = "$";
  var _prefix_2 = "~";
  var _prefix_3 = ",";
  if (_kj_p.length > 0) {
    _kj_list += _prefix_1 + "P" + _prefix_2 + _kj_p.join(_prefix_3);
  }
  if (_kj_t.length > 0) {
    _kj_list += _prefix_1 + "T" + _prefix_2 + _kj_t.join(_prefix_3);
  }
  if (_kj_s.length > 0) {
    _kj_list += _prefix_1 + "S" + _prefix_2 + _kj_s.join(_prefix_3);
  }
  if (_kj_a.length > 0) {
    _kj_list += _prefix_1 + "A" + _prefix_2 + _kj_a.join(_prefix_3);
  }
  
  if (_kj_list.length > 0) {
    try {
      _kj_list = _kj_list.substring(1);
//       console.log(_kj_list);
    
      $.ajax({
        url: "/ilos/co/course_unread_list.acl",
        type: "POST",
        data: {
          "chkList" : _kj_list
        , "_prefix_1" : _prefix_1  
        , "_prefix_2" : _prefix_2  
        , "_prefix_3" : _prefix_3  
        ,  encoding : "utf-8"
        },
        async: true,
        success: function(data){
//           console.log(data);
          if (data) {
            if (data.isError) {
//               alert(data.isError);
            } else {
//               $('.unread_icon_new').remove();
              $('.course_unread_cnt').remove();
              
              var item_list = data.item_list;
//               console.log(item_list);
              if (item_list && item_list.length > 0) {
                var _item;
                for (var i = 0; i < item_list.length; i++) {
                  _item = item_list[i];
                  var _kj = _item.KJKEY;
                  var _kj_auth = _item.KJ_AUTH;
                  var _is_unread = _item.IS_UNREAD;
                  var _unread_cnt = _item.UNREAD_CNT;
                  
                  if (_is_unread == "Y") {
//                     appendUnreadIcon(_kj_map[_kj], _unread_cnt);
                    appendUnreadCnt(_kj_map[_kj_auth + "_" + _kj], _unread_cnt);
                  }
                }
              }
              
            }
          } else {
//             console.log("Response error");
//             console.log(data);
          }
        },
        error : function(jqXHR, exception){
          ajaxErrorMsg(jqXHR, exception);
          return false;
        }
      });
    } catch (e) {
//       console.log(e);
    }
  }
}

// e : jquery element
function appendUnreadIcon(e, cnt) {
  var unread_icon_new = "";
  unread_icon_new += "<img src=\"/ilos/images/common/icon_new.png\"";
  unread_icon_new += " class=\"unread_icon_new\"";
  unread_icon_new += " title=\"Unread Item : " + cnt + "\"";
  unread_icon_new += " alt=\"Unread Item : " + cnt + "\"";
  unread_icon_new += " />";
  
  e.parent().append(unread_icon_new);
}
function appendUnreadCnt(e, cnt) {
  var course_unread_cnt = "";
  course_unread_cnt += "<img";
  course_unread_cnt += " class=\"course_unread_cnt\"";
  course_unread_cnt += " style=\"margin-top: 2px;\"";
  course_unread_cnt += " title=\"안읽은 글 : " + cnt + "\"";
  course_unread_cnt += " src= \"/ilos/images/new.png\"";
  course_unread_cnt += " >";
  
  e.parent().append(course_unread_cnt);
}
