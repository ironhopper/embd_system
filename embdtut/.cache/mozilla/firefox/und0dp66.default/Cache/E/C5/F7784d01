function popEval(){
  $.ajax({
    url: "/ilos/popup/pop_course_eval_form.acl",
    type: "POST",
    data: { 
      encoding  : "utf-8"
    },
    async: false,
    success: function(data){
      if(data.isError){
        alert(data.message);
      }
      else{
        $("#dialog-course-eval").html(data);
        $("#dialog-course-eval").dialog("open");
      }
    },
    error : ajaxErrorMsg
  });
}


//수강과목 목록
function popEvalCourseList(org_dv, online_flag, eval_seq){
  $.ajax({
    url: "/ilos/popup/pop_course_eval_list1.acl",
    type: "POST",
    data: {
      ORG_DV      : org_dv,
      ONLINE_FLAG : online_flag,
      EVAL_SEQ    : eval_seq,
      encoding    : "utf-8"
    },
    async: false,
    success: function(data){
      if(data.isError){
        alert(data.message);
      }
      else{
        $('html, body').scrollTop(0);
        $('div[name="EVAL_TITLE"]').show();
        $('div[name="EVAL_INFO"]').show();
        $('div[name="COURSE_LIST"]').show();
        $("#pop_eval_list_"+eval_seq).html(data);
      }
    },
    error : ajaxErrorMsg
  });
}

//설문문항 
function popEvalList(org_dv, eval_seq, kjkey, complete_yn, emp_no){
  if(complete_yn == 'Y'){
    alert('Lecture evaluation is completed course');
    return;
  }
  else{
    $.ajax({
      url: "/ilos/popup/pop_course_eval_list2.acl",
      type: "POST",
      data: { 
        ORG_DV   : org_dv,
        EVAL_SEQ : eval_seq,
        KJKEY    : kjkey,
        EMP_NO   : emp_no,
        encoding : "utf-8"
      },
      async: false,
      success: function(data){
        if(data.isError){
          alert(data.message);
        }
        else{
          $('div[name="EVAL_TITLE"]').hide();
          $('div[name="EVAL_INFO"]').hide();
          $('#EVAL_'+eval_seq).show();
          $('#EVAL_INFO_'+eval_seq).show();
          $('div[name="COURSE_LIST"]').hide();
          $("#pop_eval_list_"+eval_seq).html(data);
        }
      },
      error : ajaxErrorMsg
    });
  }
}

function popEvalSave(org_dv, eval_seq, kjkey, emp_no, online_flag, dcSize){
  var q_id = [];
  var q_tp = [];
  var q_as = [];
  var q_no = [];
  var temp_as = '';
  var temp_no = '';
  
  for(var a=1; a-1<dcSize; a++){
    temp_as = '';
    temp_no = '';
    q_id.push($("#QUESTION_ID_"+a).attr("QUESTION_ID"));
    q_tp.push($("#QUESTION_ID_"+a).attr("QUESTION_KIND"));
    if("3" == $("#QUESTION_ID_"+a).attr("QUESTION_KIND")){
      $("input:radio[name='"+a+"_EX_NO']").each(function(index){
        if($(this).is(":checked")) {
          temp_as = $(this).val();
          temp_no = $(this).attr("QUESTION_NO");
        }
      });
      q_as.push(temp_as);
      q_no.push(temp_no);
    }
    else{
      q_as.push($("#TXT_"+a).val());
      q_no.push("");
    }
  }
  
  for(var a in q_as){
    if(trim(q_as[a]) == "") {
      alert('[Q'+Number(Number(a)+1)+']'+' Select answer');
      $("#QUESTION_ID_"+q_id[a]).focus();
      return;
    }
  }

  $.ajax({
    url: "/ilos/popup/pop_course_eval_save.acl",
    type: "POST",
    data: { 
      ORG_DV   : org_dv,
      EVAL_SEQ : eval_seq,
      KJKEY    : kjkey,
      EMP_NO   : emp_no,
      tot_q_id : q_id.join('^'),
      tot_q_tp : q_tp.join('^'),
      tot_q_as : q_as.join('^'),
      tot_q_no : q_no.join('^'),
      encoding : "utf-8"
    },
    async: false,
    success: function(data){
      if(data.isError){
        alert(data.message);
        if(data.messageCode == 'E_EVAL_FINISHED'){
          $('#dialog-course-eval').dialog('close');
        }
      }
      else{
        alert('Survey is completed');
        popEvalCourseList(org_dv, online_flag, eval_seq);
      }
    },
    error : ajaxErrorMsg
  });
}

function allComlete(){
  var flag = true;
  $("input[name='all_comlete']").each(function(){
    if($(this).val() == 'false'){
      flag = false;
    }
  });
  
  if(flag) $("#pop_eval_close_btn2").show();
}
