<script type="text/javascript" src="/ilos/js/jquery.form.js?v=20200820" ></script>

<script>
$(function() {  
  cmmtList("2524943","K002","KC02","630","st","");
});


$(function() {  
});

function cmmtList(menu_seq,brd_id,cmmt_brd_id,width,auth,menu_seq2) {
  $.ajax({
    url: "/ilos/co/cmmt_list.acl",
    type: "POST",
    data: { 
      KJKEY    : "A2020318407941001",
      MENU_SEQ : menu_seq,
      MENU_SEQ2 : menu_seq2,
      BRD_ID : brd_id,
      CMMT_BRD_ID : cmmt_brd_id,
      WIDTH : width,
      auth : auth,
      encoding : "utf-8"
    },
    async: false,
    success: function(data){
      $("#commentbox_"+menu_seq).html(data);
    },
    error : function(jqXHR, exception){
      ajaxErrorMsg(jqXHR, exception);
      return false;
    }
  });
}
function insertCmmt(menu_seq,brd_id,cmmt_brd_id,width,auth,menu_seq2) {
  var cm = trim($("#wrtExplain_"+menu_seq).val());
  if(!$("#file_list_"+menu_seq).attr("var")){
    if(cm.length == 0){
      alert("Please enter comments");
      $("#wrtExplain_"+menu_seq).focus();
      return;
    }    
  }

  if(getLength(cm)  > 1800 ) {
    alert(imaxsoft.i18n.Resource.getString('err.cmmtLimitSize',{'$size':getLength(cm)+'byte'}));
    $("#wrtExplain_"+menu_seq).focus();
    return;
  }
      
  $.ajax({
    url: "/ilos/co/cmmt_insert.acl",
    type: "POST",
    data: { 
      BRD_ID : brd_id,
      auth : auth,
      MENU_SEQ : menu_seq,
      MENU_SEQ2 : menu_seq2,
      FILE_SEQS : $("#file_list_"+menu_seq).attr("var"),
      CMT : cm,
      encoding : "utf-8"
    },
    async: false,
    success: function(data){
      cmmtList(menu_seq,brd_id,cmmt_brd_id,width,auth,menu_seq2);
    },
    error : function(jqXHR, exception){
      ajaxErrorMsg(jqXHR, exception);
      return false;
    }
  });
}

function updateCmmt(num,menu_seq,brd_id,cmmt_brd_id,width,auth,content_seq,menu_seq2) {
  var _textarea = $("#wrtExplain_"+menu_seq+num);
  var cm = trim(_textarea.val());
  
  if(cm.length == 0){
    alert("Please enter comments");
    _textarea.focus();
    return;
  }

  if(getLength(cm)  > 1800 ) {
    alert(imaxsoft.i18n.Resource.getString('err.cmmtLimitSize',{'$size':getLength(cm)}));
    _textarea.focus();
    return;
  }
  $.ajax({
    url: "/ilos/co/cmmt_update.acl",
    type: "POST",
    data: {
      BRD_ID : brd_id,
      auth : auth,
      CMMT_NUM : num,
      MENU_SEQ : menu_seq,
      MENU_SEQ2 : menu_seq2,
      CONTENT_SEQ : content_seq,
      FILE_SEQS : $("#file_list_"+menu_seq+num).attr("var"),
      D_FILE_SEQS : $("#file_list_"+menu_seq+num).attr("var") == $("#file_list_"+menu_seq+num).attr("var2") ? "" : $("#file_list_"+menu_seq+num).attr("var2"),
      CMT : cm,
      encoding : "utf-8"
    },
    async: false,
    success: function(data){
      cmmtList(menu_seq,brd_id,cmmt_brd_id,width,auth,menu_seq2);
    },
    error : function(jqXHR, exception){
      ajaxErrorMsg(jqXHR, exception);
      return false;
    }
  });
  
}

function deleteCmmt(num,menu_seq,brd_id,cmmt_brd_id,width,auth,content_seq,menu_seq2) {
  if(!confirm("Are you sure to delete?")){
    return false;
  }

  $.ajax({
    url: "/ilos/co/cmmt_delete.acl",
    type: "POST",
    data: { 
      BRD_ID : "K002",
      auth : "st",
      CMMT_NUM : num,
      MENU_SEQ : menu_seq,
      MENU_SEQ2 : menu_seq2,
      CONTENT_SEQ : content_seq,
      encoding : "utf-8"
    },
    async: false,
    success: function(data){
      cmmtList(menu_seq,brd_id,cmmt_brd_id,width,auth,menu_seq2);
    },
    error : function(jqXHR, exception){
      ajaxErrorMsg(jqXHR, exception);
      return false;
    }
  });
}

function resetEditCmmt(num,menu_seq) {
  if (num) {
    $('#cmt_edit_wrap_'+num).hide();
    $('#cmt_txt_'+num).show();
    $('#cmmt_edit'+num).removeClass('cmmt_edit_on');
  } else {
    $('.cmmt_edit_on').removeClass('cmmt_edit_on');
    $('.comment-edit').hide();
    $('.comment-addr').show();
  }
  $("#edit_span1"+num).show();
  $("#edit_span2"+num).hide();
}
function editCmmt(num,menu_seq) {
  $(".file_class").remove();
  $(".file_btn").show();
  var editBtn = $('#cmmt_edit'+num);
  if (editBtn.hasClass('cmmt_edit_on')) {
    return false;
  } else {
    var _txt = $('#cmt_txt_'+num);
    var _txt_val = $('#cmt_txt_val_'+num).val();
    var _editor = $('#wrtExplain_'+menu_seq+num);
    resetEditCmmt();
    editBtn.addClass('cmmt_edit_on');
    _editor.val(_txt_val);
    _lec(_editor);
    _txt.hide();
    $('#cmt_edit_wrap_'+num).show();
    _editor.focus();
    $(".edit_span1").show();
    $(".edit_span2").hide();
    $("#edit_span1"+num).hide();
    $("#edit_span2"+num).show();
  }
}
function _lec(obj){
  obj.parent().find(".ext_len").html(getLength(trim(obj.val())));
}
function addFileDiv(menu_seq,file_seq,file_nm,file_size,flag,preview_img,num){
  if(!num){
    num = "";
  }else{
    if(!$("#file_list_"+menu_seq+num).attr("var2")){
      $("#file_list_"+menu_seq+num).attr("var2",file_seq);  
    }
  }
  var txt = '<div id="file_div_'+file_seq+'" class="file_class">'+
              '<img style="margin:0;" alt="'+ (flag == 1 ? "Image" : flag == 2 ? "Voice" : "File")+'" src="/ilos/images/common/cmmt/'+ (flag == 1 ? "image2.png" : flag == 2 ? "voice2.png" : "file3.png")+'">&nbsp;'+
                file_nm+'('+file_size+'byte)'+
              '&nbsp;<img onclick="delFileDiv(\''+file_seq+'\',\''+num+'\',\''+menu_seq+'\');" style="margin: 0 5px; cursor:pointer;" alt="Delete" src="/ilos/images/common/cmmt/del.png">'+
            '</div>';
  $("#file_btn_"+menu_seq+num).hide();
  $("#file_list_"+menu_seq+num).append(txt);
  $("#file_list_"+menu_seq+num).attr("var",file_seq);
}
function delFileDiv(file_seq,num,menu_seq){
  if(!num){
    num = "";
  }else if($("#file_list_"+menu_seq+num).attr("var2") == file_seq){
    $("#file_div_"+file_seq).remove();
    $("#file_btn_"+menu_seq+num).show();
    $("#file_list_"+menu_seq+num).attr("var","");
    return;
  }
  $.ajax({
     url: "/ilos/co/file_delete.acl",
     type: "POST",
     dataType: 'json',
     data: { 
       FILE_SEQ : file_seq,
       encoding : "utf-8"
     },
     success: function(data){
       $("#progress").hide();
       if(data.isError) {
         alert(data.message);
       }
       else {
         $("#file_div_"+file_seq).remove();
         $("#file_btn_"+menu_seq+num).show();
         $("#file_list_"+menu_seq+num).attr("var","");
       }
     },
     error : function(jqXHR, exception){
       ajaxErrorMsg(jqXHR, exception);
       $("#progress").hide();
       document.location.href="/ilos";
     }
  });
}


</script>
