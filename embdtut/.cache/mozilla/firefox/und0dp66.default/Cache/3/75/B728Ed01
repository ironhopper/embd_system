<script type="text/javascript">
$(function() {  
  answerlist();
  listPage("");
  
  $("#SCH_CHECK1").change(function(){
    listPage("");
  });
  $("#SCH_CHECK2").change(function(){
    listPage("");
  });
  $("#SCH_CHECK3").change(function(){
    listPage("");
  });
  
  $("#sch_btn").click(function() {
    listPage("");
  });
  
  $("#SCH_VALUE").keypress(function(event) {
    if(event.which == 13) {
      listPage("");
    }
  });

  $("#deleteBtn").click( function() {
    if(!confirm("Are you sure to delete?")) {
      return;
    }
        
    $.ajax({
      url: "/ilos/st/course/qna2_delete.acl",
      type: "POST",
      data: { 
        ud : "13184322",
        ky : "A2020318407941001",
        returnData : "json",
        ARTL_NUM : "2524943", 
        encoding : "utf-8"
            },
      async: false,
      success: function(data){
          if(data.isError){
            alert(data.message);
            pfRoomAuthCheck(data);
          }else{
            listMnu();
          }
        },
      error : ajaxErrorMsg
    });
  });  //deleteBtn


});

function answerlist(){
  $.ajax({
      url: "/ilos/st/course/qna2_answer_form.acl",
    type: "POST",
    data: { 
      ud : "13184322",
      ky : "A2020318407941001",
      ARTL_NUM : "2524943",
      encoding : "utf-8"
          },
    async: false,
    success: function(html){
            $("#answer_list").html(html);
      },
    error : ajaxErrorMsg
  });
}

function listMnu() {
  document.location.href="/ilos/st/course/qna2_faq_form.acl?SCH_KEY=&SCH_CHECK1=&SCH_CHECK2=&SCH_CHECK3=&SCH_VALUE=&display=&start=";
}

function pageMove(url){
  if(stSessionRoomCheck("13184322","A2020318407941001")){
    return;
  }
  document.location.href = url;
}

function deleteReply(paramURL, ARTL_NUM, Q_ARTL_NUM) {
  if(!confirm("Are you sure to delete?")) {
    return;
  }

  $.ajax({
    url: paramURL,
    type: "POST",
    data: { 
      ud : "13184322",
      ky : "A2020318407941001",
      returnData : "json",
      ARTL_NUM : ARTL_NUM, 
      Q_ARTL_NUM : Q_ARTL_NUM,   
      encoding : "utf-8"
          },
    async: false,
    success: function(data){
        if(data.isError){
          alert(data.message);
          pfRoomAuthCheck(data);
        }else{
          document.location.href="/ilos/st/course/qna2_view_form.acl?SCH_KEY=&SCH_CHECK1=&SCH_CHECK2=&SCH_CHECK3=&SCH_VALUE=&display=&start=&ARTL_NUM=2524943";
        }
      },
    error : ajaxErrorMsg
  }); 
}

// 151209 추천이벤트 변경
function recommendAdd(obj) {
  var ARTL_NUM = obj.val();
  
  $.ajax({
    url: "/ilos/st/course/artl_good_insert.acl",
    type: "POST",
    data: { 
      ud : "13184322",
      ky : "A2020318407941001",
      returnData : "json",
      ARTL_NUM : ARTL_NUM,
      BRD_ID : "K002",
      encoding : "utf-8"
    },
    async: false,
    success: function(data){
      if (data.isError) {
        alert(data.message);
        pfRoomAuthCheck(data);
      } else {
        obj.removeClass("recommendAdd");
        obj.addClass("recommendCancel");
        obj.find(".good_cnt").text(Number(obj.find(".good_cnt").text())+1);
        obj.find(".img_heart").prop("src","/ilos/images/common/heart_on.png");
        //parent.answerlist();
      }
    },
    error : function(jqXHR, exception){
      ajaxErrorMsg(jqXHR, exception);
      return false;
    }
  });
}

function recommendCancel(obj) {
  var ARTL_NUM = obj.val();
  
  $.ajax({
    url: "/ilos/st/course/artl_good_cancel.acl",
    type: "POST",
    data: { 
      ud : "13184322",
      ky : "A2020318407941001",
      returnData : "json",
      ARTL_NUM : ARTL_NUM,
      BRD_ID : "K002",
      encoding : "utf-8"
    },
    async: false,
    success: function(data){
      if (data.isError) {
        alert(data.message);
        pfRoomAuthCheck(data);
      } else {
        obj.removeClass("recommendCancel");
        obj.addClass("recommendAdd");
        obj.find(".good_cnt").text(Number(obj.find(".good_cnt").text())-1);
        obj.find(".img_heart").prop("src","/ilos/images/common/heart_off.png");
        //parent.answerlist();
      }
    },
    error : function(jqXHR, exception){
      ajaxErrorMsg(jqXHR, exception);
      return false;
    }
  });
}


function listPage(start) {
  if(typeof start == 'undefined') start = 1;
    var chk1;      //내 질문
    var chk2;      //해결 질문
    var chk3;      //미해결 질문
    var chk1_2;    //내, 해결 질문
    var chk1_3;    //해결, 미해결 질문
    
    if($("#SCH_CHECK1").is(":checked")) { chk1 = "Y"; } else { chk1 = "N"; }
    if($("#SCH_CHECK2").is(":checked")) { chk2 = "Y"; } else { chk2 = "N"; }
    if($("#SCH_CHECK3").is(":checked")) { chk3 = "Y"; } else { chk3 = "N"; }
    
    if(chk1 == "Y" && chk2 == "Y") chk1_2 = "Y";
    if(chk1 == "Y" && chk3 == "Y") chk1_3 = "Y";
    if(chk2 == "Y" && chk3 == "Y") chk2_3 = "Y";    
    
    if(chk1 == "Y" && chk2 == "Y" && chk3 == "Y"){
      chk1 = "Y";
      chk2 = "N";
      chk3 = "N";
      chk1_2 = "N";
      chk1_3 = "N";
    }
    if(chk2 == "Y" && chk3 == "Y") {
      chk1 = "N"; 
      chk2 = "N";
      chk3 = "N";
      chk1_2 = "N";
      chk1_3 = "N";
    }
    if(chk1_2 == "Y" || chk1_3 == "Y") {
      chk1 = "N"; 
      chk2 = "N";
      chk3 = "N";
    }

  $.ajax({
    url: "/ilos/st/course/qna2_list.acl",
    type: "POST",
    data: {
      chk1   : chk1,    //내 질문
      chk2   : chk2,    //해결 질문
      chk3   : chk3,    //미해결 질문
      chk1_2 : chk1_2,  //내, 해결 질문
      chk1_3 : chk1_3,  //해결, 미해결 질문
      SCH_CHECK1 : chk1,
      SCH_CHECK2 : chk2,
      SCH_CHECK3 : chk3,
      ODR :  getSortString("sortable"),
      ARTL_NUM : "2524943",
      SCH_VALUE : $("#SCH_VALUE").val(),
      start : start,
      encoding : "utf-8"
    },
    async: false,
    success: function(data) {
      $("#list_zone").html(data);
    },
    error: ajaxErrorMsg
  });
}
function updateQnaStatus(num, kjkey, brd, obj){
  var status_btn = $(obj).attr("st");
  var status;
  if(status_btn == "Y"){
    status = "N";
  }else{
    status = "Y";
  }
   $.ajax({
    url: "/ilos/st/course/qna2_prostatus_update.acl",
    type: "POST",
    data: { 
      ARTL_NUM   : num,
      KJKEY      : kjkey,
      BRD_ID     : brd,
      PRO_STATUS : status,
      encoding : "utf-8"
    },
    async: false,
    success: function(data){
      if (data.isError) {
        alert(data.message);
        pfRoomAuthCheck(data);
      } else {
        if(status_btn == "Y"){
          $(obj).attr("st","N");
          $("#faq_txt").text("Was the question solved?");
          $("#status_bnt_N").show();
          $(obj).addClass("site-background-color");
          $("#status_bnt_Y").hide();
          $(obj).removeClass("status_Y_color");
        }else{
          $(obj).attr("st","Y");
          $("#faq_txt").text("Solved Questions");
          $("#status_bnt_Y").show();
          $(obj).addClass("status_Y_color");
          $("#status_bnt_N").hide();
          $(obj).removeClass("site-background-color");
        }
        listPage("");
      }
    },
    error : ajaxErrorMsg
  });
}
</script>
