<script type="text/javascript">
$(function(){
  $("[name=user_info_pop]").css("cursor","pointer").css("font-weight","bold").css("letter-spacing","0");
  $.fx.speeds._default = 500;
  $( "#popup_room" ).dialog({
    autoOpen: false,
    hide: "fade", //explode"
    modal: true,
    width:800,
    resizable: false,
    position: {
      using: function() {
        var top = "10";
        var left = Math.max(0, (($(window).width() - $(this).outerWidth()) / 2) + $(window).scrollLeft());

        $(this).css("top", top + "px");
        $(this).css("left", left + "px");
        $(this).css("marginBottom", top + "px");
      }
    }    
  });
});
//교수 과목용
function userInfoShow(user_id, kjkey, year, term, startTab, viewMode){
  if (!viewMode) {
    viewMode = 0;
  }
  $.ajax({
    url: "/ilos/co/user_info_pop_form.acl",
    type: "POST",
    data: {
      USER_ID : user_id,
      kjkey : kjkey,
      kj_year : year,
      kj_term : term,
      START_TAB : startTab,
      VIEW_MODE : viewMode,
      encoding : "utf-8"
    },
    async: false,
    success: function(html){
      if (html && html.message) {
        alert(html.message);
        return;
      } else {
        $("#popup_room").html(html);
        $( "#popup_room" ).dialog('option', 'title', "User info");
        $( "#popup_room" ).dialog( "open" );
      }
    },
    error : ajaxErrorMsg
  });
}
//커뮤니티 및 마이페이지용
function userInfoShow2(user_id, kjkey, year, term){
  $.ajax({
    url: "/ilos/co/user_info2_pop_form.acl",
    type: "POST",
    data: {
      USER_ID : user_id,
      kjkey : kjkey,
      kj_year : year,
      kj_term : term,
           encoding : "utf-8"
          },
    async: false,
    success: function(html){
      if (html && html.message) {
        alert(html.message);
        return;
      } else {
        $("#popup_room").html(html);
        $( "#popup_room" ).dialog('option', 'title', "User info");
        $( "#popup_room" ).dialog( "open" );
      }
    },
    error : ajaxErrorMsg
  });
}
