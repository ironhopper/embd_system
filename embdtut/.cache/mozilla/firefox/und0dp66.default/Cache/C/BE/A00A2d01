function callback () {
  $(".term").removeClass("termBg");
  $("#subject_room").hide("blind",300);
};

function eclassSubjectChange(url){
  if($("#subject_room").css("display") == 'none'){
    if($(".subject-main").size() == '0') {
      $.ajax({
        url: "/ilos/st/course/eclass_room_search_form.acl",
        type: "POST",
        data: {
               ROOM_SCH_YEAR : $("#KJ_YEAR").val(),
               ROOM_SCH_TERM : $("#KJ_TERM").val(),
               ROOM_SCH_KEY : $("#KJ_KEY").val(),
               returnURI : $("#returnURI").val(),
               encoding : "utf-8"
              },
        async: false,
        success: function(html){
                $("#subject_room").html(html);
          },
          error : ajaxErrorMsg
      });
    }
    $(".term").addClass("termBg");
    $("#subject_room").show("blind",300);
  }
//   else {
//     callback();
//   }
}


function checkCourseMenuUnreadCnt(menu_ids) {
  
  var isRefreshTargetOnly = true;
  
  if (!menu_ids) {
    menu_ids = "'plan','lecture_weeks','notice','qna','lecture_material','attend','report','project','test','discuss','clicker','survey','eval','open_material'";
    isRefreshTargetOnly = false;
  } else {
    menu_ids = trim(menu_ids);
  }
  if (menu_ids.length == 0) {
    return;
  }
  try {
    $.ajax({
      url: "/ilos/co/course_unread_menu_cnt_list.acl",
      type: "POST",
      data: {
        MENU_IDS : menu_ids
      , encoding : "utf-8"
      },
      async: true,
      success: function(data){
//         console.log(data);
        if (data) {
          if (data.isError) {
//             alert(data.isError);
          } else {
            var item_list = data.item_list;
//             console.log(item_list);
            if (item_list && item_list.length > 0) {
              if (!isRefreshTargetOnly) {
                $('li .unread_menu_cnt').remove();
              } else {
                menu_ids = menu_ids.replaceAll("'", "");
                if (menu_ids.indexOf(",") > 0) {
                  var _target_menu_list = menu_ids.split(",");
                  for (var i = 0; i < _target_menu_list.length; i++) {
                    $('#menu_' + _target_menu_list[i] + ' .unread_menu_cnt').remove();
                  }
                } else {
                  $('#menu_' + menu_ids + ' .unread_menu_cnt').remove();
                }
              }
              var _item;
              for (var i = 0; i < item_list.length; i++) {
                _item = item_list[i];
                var _menu_id = _item.MENU_ID;
                var _unread_cnt = _item.UNREAD_CNT;
                var _artl_grp_id = _item.ARTL_GRP_ID;
                
                if (_unread_cnt > 0) {
                  appendUnreadCnt(_menu_id, _unread_cnt, _artl_grp_id);
                }
              }
            }
            
          }
        } else {
//           console.log("Response error");
//           console.log(data);
        }
      },
      error : function(jqXHR, exception){
        ajaxErrorMsg(jqXHR, exception);
        return false;
      }
    });
  } catch (e) {
//     console.log(e);
  }
  
}

function appendUnreadCnt(menu_id, cnt, artl_grp_id) {
  var unread_menu_cnt = "<span class=\"unread_menu_cnt\">" + cnt + "</span>"
  if (menu_id == "open_material" && artl_grp_id) {
    $('li' + ' > a#menu_open_material_' + artl_grp_id).append(unread_menu_cnt);
  } else {
    $('li' + ' > a#menu_' + menu_id).append(unread_menu_cnt);
  }
}


