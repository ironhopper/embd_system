        encoding : "utf-8"
            },
    success: function(data){
            if(data.isError) {
              //읽지않은 쪽지 없음. alert(data.message);
              $("#message_cnt").hide();
              $("#message_cnt").text("");
            }
            else {
              if(data.records[0].NEW_MSG_CNT > 0) {
                $("#message_cnt").text(data.records[0].NEW_MSG_CNT);
                $("#message_cnt").show();
              } else {
                $("#message_cnt").hide();
                $("#message_cnt").text("");
              }
            }  
      },
    error : ajaxErrorMsg
