function filmIconClick(artl_num) {
  if(stSessionRoomCheck("13184322","A2020318407941001")){
    return;
  }
  document.location.href="/ilos/st/course/lecture_material_online_view_form.acl?LECTURE_WEEKS="+artl_num+"&SCH_KEY=&SCH_VALUE=&display=&start=";
}
function cameraClick(artl_num) {
  if(stSessionRoomCheck("13184322","A2020318407941001")){
    return;
  }
  document.location.href="/ilos/st/course/lecture_material_view_form.acl?ARTL_NUM="+artl_num+"&SCH_KEY=&SCH_VALUE=&display=&start=";
}
function downloadClick(artl_num) {
  $.ajax({
    url: "/ilos/co/list_file_list2.acl",
    type: "POST",
    data: { 
      ud : "13184322",
      ky : "A2020318407941001",
      pf_st_flag : "2",
      CONTENT_SEQ : artl_num,
      encoding : "utf-8"
          },
    async: false,
    success: function(html){
            $("#file_list_"+artl_num  ).html(html);
      },
    error : ajaxErrorMsg
  });
}


function listPage(start){
  if(stSessionRoomCheck("13184322","A2020318407941001")){
    return;
  }
  
  circleLoaderPrepend("material_list");
  $.ajax({
    url: "/ilos/st/course/lecture_material_list.acl",
    type: "POST",
    data: {
      start : start,
      display : "1",
      SCH_VALUE : $("#SCH_VALUE").val(),
      ud : "13184322",
      ky : "A2020318407941001",
      encoding : "utf-8"
          },
    async: true,
    success: function(data){
          $("#material_list").html(data);
          circleLoaderRemove("material_list");
      },
    error : ajaxErrorMsg
  });
}
function pageMove(url, e){
  e = e || window.event;
  var target = e.target || e.srcElement;
  
  if(target.tagName != 'IMG'){
    if(target.tagName != 'A'){    
      if(stSessionRoomCheck("13184322","A2020318407941001")){
        return;
      }
     document.location.href = url;
    }
  }
}
